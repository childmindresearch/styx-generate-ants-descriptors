{
  "tool-version": "7.4.1",
  "name": "reconbatchjobs",
  "author": "FreeSurfer Developers",
  "description": "Batch job processor for reconstruction scripts",
  "command-line": "reconbatchjobs [LOGFILE] [CMDFILES]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Log file to capture output of batch jobs",
      "value-key": "[LOGFILE]",
      "type": "String",
      "optional": false,
      "id": "logfile",
      "name": "Log file"
    },
    {
      "description": "Command files for batch processing",
      "value-key": "[CMDFILES]",
      "type": "String",
      "list": true,
      "optional": false,
      "id": "cmdfiles",
      "name": "Command files"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}