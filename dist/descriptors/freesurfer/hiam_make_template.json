{
  "tool-version": "7.4.1",
  "name": "hiam_make_template",
  "description": "This program adds a template into an average surface using FreeSurfer.",
  "command-line": "/usr/local/freesurfer/bin/hiam_make_template [HEMI] [SURFACE_NAME] [SUBJECTS] [OUTPUT_NAME]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Hemisphere to be processed (e.g. lh or rh)",
      "value-key": "[HEMI]",
      "type": "String",
      "optional": false,
      "id": "hemi",
      "name": "Hemisphere"
    },
    {
      "description": "Name of the surface",
      "value-key": "[SURFACE_NAME]",
      "type": "String",
      "optional": false,
      "id": "surface_name",
      "name": "Surface Name"
    },
    {
      "description": "List of subject identifiers",
      "value-key": "[SUBJECTS]",
      "type": "String",
      "list": true,
      "optional": false,
      "id": "subjects",
      "name": "Subjects"
    },
    {
      "description": "Name of the output template",
      "value-key": "[OUTPUT_NAME]",
      "type": "String",
      "optional": false,
      "id": "output_name",
      "name": "Output Name"
    }
  ],
  "author": "FreeSurfer Developers",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}