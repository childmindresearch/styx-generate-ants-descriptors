{
  "tool-version": "7.4.1",
  "name": "mpr2mni305",
  "author": "FreeSurfer Developers",
  "description": "Tool for transforming MPRAGE dataset to MNI305 coordinate space",
  "command-line": "mpr2mni305 [MPR_ANAT]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "MPRAGE anatomical input file (e.g., 131)",
      "value-key": "[MPR_ANAT]",
      "type": "String",
      "optional": false,
      "id": "mpr_anat",
      "name": "MPR Anatomic Input"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}