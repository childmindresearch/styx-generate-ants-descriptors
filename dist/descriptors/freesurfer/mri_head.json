{
  "name": "mri_head",
  "command-line": "mri_head [IDENTIFY_FLAG] [READ_FLAG] [FILENAME] [HELP_FLAG]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-identify",
      "description": "Identify the MRI file",
      "value-key": "[IDENTIFY_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "identify",
      "name": "Identify Flag"
    },
    {
      "command-line-flag": "-read",
      "description": "Read the MRI file",
      "value-key": "[READ_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "read",
      "name": "Read Flag"
    },
    {
      "description": "Filename for identification or reading",
      "value-key": "[FILENAME]",
      "type": "String",
      "optional": true,
      "id": "filename",
      "name": "Filename"
    },
    {
      "command-line-flag": "-h",
      "description": "Display help information",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "help",
      "name": "Help Flag"
    },
    {
      "command-line-flag": "-u",
      "description": "Display usage information",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "usage",
      "name": "Usage Flag"
    },
    {
      "command-line-flag": "-?",
      "description": "Display help or usage information",
      "value-key": "[HELP_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "question_mark_help",
      "name": "Question Mark Help Flag"
    }
  ],
  "author": "FreeSurfer Developers",
  "description": "FreeSurfer is a software package for the analysis and visualization of neuroimaging data from cross-sectional and longitudinal studies. It is developed by the Laboratory for Computational Neuroimaging at the Martinos Center for Biomedical Imaging.\n\nFreeSurfer provides full processing streams for structural and functional MRI and includes tools for linear and nonlinear registration, cortical and subcortical segmentation, cortical surface reconstruction, statistical analysis of group morphometry, diffusion MRI, PET analysis, and much more. It is also the structural MRI analysis software of choice for the Human Connectome Project.",
  "tool-version": "7.4.1",
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}