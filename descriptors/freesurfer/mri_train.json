{
  "name": "mri_train",
  "description": "Tool for training with MRI data in FreeSurfer.",
  "command-line": "mri_train [TRAINING_FILE] [OUTPUT_FILE]",
  "schema-version": "0.5",
  "inputs": [
    {
      "id": "training_file",
      "name": "Training File",
      "type": "File",
      "value-key": "[TRAINING_FILE]",
      "description": "Input training file name",
      "optional": false
    },
    {
      "id": "output_file",
      "name": "Output File",
      "type": "String",
      "value-key": "[OUTPUT_FILE]",
      "description": "Output file",
      "optional": false
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Output file generated by mri_train.",
      "optional": false,
      "id": "output_file_generated",
      "name": "Generated Output File"
    }
  ],
  "author": "Placeholder",
  "tool-version": "1.0.0",
  "container-image": {
    "type": "docker",
    "image": "place/holder"
  }
}