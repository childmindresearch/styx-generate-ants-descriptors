{
  "name": "mris_annot_diff",
  "description": "Compare two surface annotation files. The program works with .annot only.",
  "command-line": "mris_annot_diff [ANNOT1] [ANNOT2] [DIFF_CTAB_FLAG] [VERBOSE_FLAG]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Input .annot file 1",
      "value-key": "[ANNOT1]",
      "type": "File",
      "optional": false,
      "id": "annot1",
      "name": "Annotation File 1"
    },
    {
      "description": "Input .annot file 2",
      "value-key": "[ANNOT2]",
      "type": "File",
      "optional": false,
      "id": "annot2",
      "name": "Annotation File 2"
    },
    {
      "command-line-flag": "--diff-ctab",
      "description": "Diff colortable included in .annot",
      "value-key": "[DIFF_CTAB_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "diff_ctab",
      "name": "Diff Colortable Flag"
    },
    {
      "command-line-flag": "--verbose",
      "description": "Print details of annotation/colortable differences",
      "value-key": "[VERBOSE_FLAG]",
      "type": "Flag",
      "optional": true,
      "id": "verbose",
      "name": "Verbose Flag"
    }
  ],
  "author": "Placeholder",
  "tool-version": "1.0.0",
  "container-image": {
    "type": "docker",
    "image": "place/holder"
  }
}