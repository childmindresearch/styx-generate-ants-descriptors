{
  "name": "mri_reduce",
  "tool-version": "1.0.0",
  "description": "A tool to reduce MRI file dimensions",
  "command-line": "mri_reduce [INPUT_FILE] [OUTPUT_FILE]",
  "schema-version": "0.5",
  "inputs": [
    {
      "description": "Input MRI file",
      "value-key": "[INPUT_FILE]",
      "type": "File",
      "optional": false,
      "id": "input_file",
      "name": "Input File"
    },
    {
      "description": "Output filename for the reduced MRI file",
      "value-key": "[OUTPUT_FILE]",
      "type": "String",
      "optional": false,
      "id": "output_file",
      "name": "Output File"
    }
  ],
  "output-files": [
    {
      "path-template": "[OUTPUT_FILE]",
      "description": "Reduced MRI file output",
      "optional": false,
      "id": "reduced_output",
      "name": "Reduced Output File"
    }
  ],
  "author": "Placeholder",
  "container-image": {
    "type": "docker",
    "image": "place/holder"
  }
}